<!--! Delete Alarm Dialog -->
<div id="delete-alarm-wrapper">
    <div id="delete-alarm-modal" class="reveal-modal ${modal_size}" data-reveal="">
        <a class="close-reveal-modal" href="#">&#215;</a>
        <h3 i18n:translate="">Delete Alarm</h3>
        <div class="row">
            <p i18n:translate="">Are you sure you want to delete the following alarm?</p>
        </div>
        <div class="row">
            <button id="delete-alarm-btn" class="button expand" data-service-path="${service_path}"
                ng-click="deleteAlarm($event)" i18n:translate="">
                Yes, Delete Alarm
            </button>
        </div>
    </div>
</div>
